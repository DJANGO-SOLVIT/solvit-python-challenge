def average_of_marks(n, student_marks, query_name):
    student_avg = sum(student_marks[query_name])/3
    return format(student_avg, '.2f')

if _name_ == '_main_':
    n = int(input())
    student_marks = {}
    for _ in range(n):
        name, *line = input().split()
        scores = list(map(float, line))
        student_marks[name] = scores
    query_name = input()
    result = average_of_marks(n, student_marks, query_name)
    print(result)
